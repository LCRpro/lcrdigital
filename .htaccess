RewriteEngine On
RewriteBase /

# Allow assets and robots.txt during maintenance
RewriteRule ^assets/ - [L]
RewriteRule ^robots\.txt$ - [L]
RewriteRule ^construction\.html$ - [L]
RewriteRule ^lcr-digital-agence-digital-web-seo-normandie/?$ /construction.html [L]

# Maintenance mode: serve the construction page for every request
RewriteRule ^.*$ /construction.html [L]

# SEO-friendly URLs (kept here for when maintenance mode is removed)
RewriteRule ^$ /agence-digital-site-web-rouen-normandie [R=301,L]
RewriteRule ^index\.html$ /agence-digital-site-web-rouen-normandie [R=301,L]
RewriteRule ^about\.html$ /lcr-digital-agence-digital-web-normandie-rouen [R=301,L]
