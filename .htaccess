RewriteEngine On
RewriteBase /
AddDefaultCharset UTF-8
AddCharset UTF-8 .html .php

# Allow assets and robots.txt during maintenance
RewriteRule ^assets/ - [L]
RewriteRule ^robots\.txt$ - [L]
RewriteRule ^construction\.html$ - [L]
RewriteRule ^lcr-digital-agence-digital-web-seo-normandie/?$ /construction.html [L]

# SEO-friendly URLs (kept here for when maintenance mode is removed)
RewriteRule ^$ /agence-digital-site-web-rouen-normandie [R=301,L]
RewriteCond %{THE_REQUEST} \s/+index\.html[?\s]
RewriteRule ^index\.html$ /agence-digital-site-web-rouen-normandie [R=301,L]
RewriteCond %{THE_REQUEST} \s/+about\.html[?\s]
RewriteRule ^about\.html$ /lcr-digital-agence-digital-web-normandie-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+blog\.html[?\s]
RewriteRule ^blog\.html$ /blog-actualites-digitales-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+contact\.html[?\s]
RewriteRule ^contact\.html$ /contact-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+faq\.html[?\s]
RewriteRule ^faq\.html$ /questions-frequentes-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+portfolio\.html[?\s]
RewriteRule ^portfolio\.html$ /realisations-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+portfolio_detail\.html[?\s]
RewriteRule ^portfolio_detail\.html$ /projet-detail-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+pricing\.html[?\s]
RewriteRule ^pricing\.html$ /tarifs-prestations-digitales-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+search\.html[?\s]
RewriteRule ^search\.html$ /recherche-site [R=301,L]
RewriteCond %{THE_REQUEST} \s/+service\.html[?\s]
RewriteRule ^service\.html$ /services-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+single_post\.html[?\s]
RewriteRule ^single_post\.html$ /article-agence-digitale-rouen [R=301,L]
RewriteCond %{THE_REQUEST} \s/+404_page\.html[?\s]
RewriteRule ^404_page\.html$ /page-introuvable [R=301,L]

RewriteRule ^agence-digital-site-web-rouen-normandie/?$ /index.html [L]
RewriteRule ^lcr-digital-agence-digital-web-normandie-rouen/?$ /about.html [L]
RewriteRule ^blog-actualites-digitales-rouen/?$ /blog.html [L]
RewriteRule ^contact-agence-digitale-rouen/?$ /contact.html [L]
RewriteRule ^questions-frequentes-agence-digitale-rouen/?$ /faq.html [L]
RewriteRule ^realisations-agence-digitale-rouen/?$ /portfolio.html [L]
RewriteRule ^projet-detail-agence-digitale-rouen/?$ /portfolio_detail.html [L]
RewriteRule ^tarifs-prestations-digitales-rouen/?$ /pricing.html [L]
RewriteRule ^recherche-site/?$ /search.html [L]
RewriteRule ^services-agence-digitale-rouen/?$ /service.html [L]
RewriteRule ^service-detail-agence-digitale-rouen/?$ /service-site-vitrine [R=301,L]
RewriteRule ^service-one-page/?$ /service_one_page.html [L]
RewriteRule ^service-site-vitrine/?$ /service_site_vitrine.html [L]
RewriteRule ^service-site-catalogue/?$ /service_site_catalogue.html [L]
RewriteRule ^service-ecommerce/?$ /service_ecommerce.html [L]
RewriteRule ^service-app-mobile/?$ /service_app_mobile.html [L]
RewriteRule ^service-sur-mesure/?$ /service_sur_mesure.html [L]
RewriteRule ^article-agence-digitale-rouen/?$ /single_post.html [L]
RewriteRule ^page-introuvable/?$ /404_page.html [L]
